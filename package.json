{
  "name": "ebdp1-plus-base-structure-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:init": "node ./db-init.js & typeorm-ts-node-commonjs --d ./src/db/data-source.ts migration:run",
    "db:migration:create": "typeorm-ts-node-commonjs migration:create ./src/db/migrations",
    "db:migration:generate": "typeorm-ts-node-commonjs migration:generate --dataSource=./src/db/data-source.ts ./src/db/migrations",
    "start": "ts-node ./index.ts",
    "build": "tsc -p tsconfig.json",
    "typeorm": "typeorm-ts-node-commonjs migration:run --help",
    "test": "jest --runInBand --silent --config ./jest.config.js ",
    "test:db:init": "cross-env NODE_ENV=test && node ./db-init.js && typeorm-ts-node-commonjs migration:run --dataSource=src/db/data-source.ts"
  },
  "dependencies": {
    "@ebdp1/config-lib": "git+ssh://git@github.com/InformaIPCo/ebdp1-config-lib.git#manual_version_1.1.1",
    "docker-compose": "^0.23.17",
    "fastify": "^4.9.2",
    "fastify-autoload": "^3.13.0",
    "fastify-cli": "^5.5.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10",
    "typeorm-naming-strategies": "^4.1.0",
    "node-notifier": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "jest": "^29.2.1",
    "jest-watch-master": "^1.0.0",
    "jest-watch-repeat": "^2.0.0",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
